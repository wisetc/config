{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "components/*": [
        "./components/*"
      ],
      "src/*": [
        "./*"
      ]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
